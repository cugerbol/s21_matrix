#test s21_eq_matrix_test_1
matrix_t A;
matrix_t B;
s21_create_matrix(3, 3, &A);
s21_create_matrix(3, 3, &B);
s21_init_matrix(1, &A);
s21_init_matrix(1, &B);
int res = s21_eq_matrix(&A, &B);
ck_assert_int_eq(res, 1);
s21_remove_matrix(&A);
s21_remove_matrix(&B);

#test s21_eq_matrix_test_2
matrix_t A;
matrix_t B;
s21_create_matrix(3, 3, &A);
s21_create_matrix(3, 3, &B);
s21_init_matrix(2, &A);
s21_init_matrix(1, &B);
int res = s21_eq_matrix(&A, &B);
ck_assert_int_eq(res, 0);
s21_remove_matrix(&A);
s21_remove_matrix(&B);

#test s21_eq_matrix_test_3
matrix_t A;
matrix_t B;
s21_create_matrix(2, 3, &A);
s21_create_matrix(3, 3, &B);
s21_init_matrix(1, &A);
s21_init_matrix(1, &B);
int res = s21_eq_matrix(&A, &B);
ck_assert_int_eq(res, 0);
s21_remove_matrix(&A);
s21_remove_matrix(&B);

#test s21_eq_matrix_test_4
matrix_t A;
s21_create_matrix(2, 3, &A);
s21_init_matrix(1, &A);
int res = s21_eq_matrix(&A, NULL);
ck_assert_int_eq(res, 0);
s21_remove_matrix(&A);

#test s21_eq_matrix_test_5
matrix_t B;
s21_create_matrix(3, 3, &B);
s21_init_matrix(1, &B);
int res = s21_eq_matrix(NULL, &B);
ck_assert_int_eq(res, 0);
s21_remove_matrix(&B);